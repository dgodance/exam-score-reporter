{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/feedback/index",
      "style": {
        "navigationBarTitleText": "反馈列表"
      }
    },
    {
      "path": "pages/compare/index",
      "style": {}
    },
    {
      "path": "pages/chart/index",
      "style": {}
    },
    {
      "path": "pages/report/index",
      "style": {}
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/subjectSetting/index",
      "style": {
        "navigationBarTitleText": "课程配置"
      }
    }

  ],
  "tabBar": {
    "color": "#919191",
    "selectedColor": "#FF6699",
    "backgroundColor": "#fff",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "/static/images/home.png",
        "selectedIconPath": "/static/images/home-active.png"
      },
      {
        "pagePath": "pages/report/index",
        "text": "成绩登记",
        "iconPath": "/static/images/report.png",
        "selectedIconPath": "/static/images/report-active.png"
      },
      {
        "pagePath": "pages/chart/index",
        "text": "可视化",
        "iconPath": "/static/images/chart.png",
        "selectedIconPath": "/static/images/chart-active.png"
      },
      {
        "pagePath": "pages/my/index",
        "text": "我的",
        "iconPath": "/static/images/my.png",
        "selectedIconPath": "/static/images/my-active.png"
      }
    ]
  },
  "sitemapLocation": "sitemap.json",
  "globalStyle": {
    "backgroundColor": "#F6F6F6",
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#F6F6F6",
    "navigationBarTitleText": "考试成绩记录",
    "navigationBarTextStyle": "black",
    "enablePullDownRefresh": true,
    "onReachBottomDistance": 60,
    "usingComponents": {}
  },
  "subPackages": []
}
